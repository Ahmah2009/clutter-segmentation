  <arg name="ltopic" default="/narrow_stereo_textured/left/image_raw" />
  <arg name="rtopic" default="/narrow_stereo_textured/right/image_raw" />
  <arg name="train" default="/home/alex/cturtle_trunk/wg-ros-pkg-unreleased/stacks/object_recognition/textured_object_detection/bin/train_cf" />    
  <arg name="config" default="/home/alex/cturtle_trunk/wg-ros-pkg-unreleased/stacks/object_recognition/textured_object_detection/camera/prosilica.config" />  
